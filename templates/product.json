        "product_height": 500,
        "video_looping": false,
        "gallery_arrows": true,
        "enable_zoom": true,
        "enable_product_lightbox": true,
        "slideshow_speed": 0,
        "slideshow_transition": "slide",
        "display_thumbnails": true,
        "thumbnail_position": "bottom-thumbnails",
        "enable_thumbnail_slider": true,
        "width": "standard",
        "padding_top": 20,
        "padding_bottom": 20,
        "animation": "none",
        "css_class": "",
        "custom_css": ""
      }
    }
  },
  "order": [
    "breadcrumbs",
    "sidebar",
    "main"
  ]
